defence = 100        ;8.9775 100% defensive power (more is stronger)
fall.defence_up = 90 ;Def goes up by 50% everytime player is hit on ground
liedown.time =11     ;Time player takes before getting up from HIT_LIEDOWN
airjuggle = 15       ;Can do 14 points of juggling
sparkno = 0          ;Default spark number
guard.sparkno = 0    ;Default guard spark number
volume = 0
IntPersistIndex=1
FloatPersistIndex=0
KO.echo = 0          ;1 to enable echo on KO
power = 3000

[Size]
